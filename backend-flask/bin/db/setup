#! /usr/bin/bash 

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="db-setup"
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"

schema_path="$(realpath .)/backend-flask/bin/db"
echo $schema_path

source "$schema_path/drop"
source "$schema_path/create"
source "$schema_path/schema-load"
source "$schema_path/seed"
source "$schema_path/update_cognito_user_ids"